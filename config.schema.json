{
  "pluginAlias": "FanSwitch",
  "pluginType": "accessory",
  "singular": false,
  "schema": {
    "type": "object",
    "properties": {
      "name": {
        "title": "Name",
        "type": "string",
        "required": true
      },
      "ip": {
        "title": "IP address",
        "type": "string",
        "required": true
      },
      "password": {
        "title": "Password",
        "type": "string",
        "required": true
      },
      "minSpeed": {
        "title": "Min fan speed",
        "type": "integer",
				"default": 0,
				"maximum": 9,
				"minimum": 0,
				"required": false
      },
      "maxSpeed": {
        "title": "Max fan speed",
        "type": "integer",
				"default": 10,
				"maximum": 10,
				"minimum": 1,
				"required": false
      }
    }
  }
}